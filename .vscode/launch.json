  "version": "0.2.0",
  "console": "integratedTerminal",
  "configurations": [
    {
      "name": "Launch ElmahCore.Demo3",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}\\ElmahCore.DemoCore3\\bin\\Debug\\netcoreapp3.1\\ElmahCore.DemoCore3.dll",
      "args": [],
      "cwd": "${workspaceFolder}\\ElmahCore.DemoCore3\\",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/ElmahCore.DemoCore3/Views"
      }
    },
    {
      "name": "Launch ElmahCore.Demo5",
